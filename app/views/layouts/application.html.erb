<!DOCTYPE html>
<html>
<head>
  <title>ABBlog</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if current_user %>
  <div>
    <div>
      <%= link_to('Deconnect', destroy_user_session_path, method: :delete)%>
    </div>
    <%= current_user.email %>/<%= current_user.last_name %>/<%= current_user.first_name %>
  </div>
<% end %>
<%= yield %>

</body>
</html>
